<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="Practice1DBDAOCheckExistRSQL">
			<desc><![CDATA[Check exist msg_code]]></desc>
			<sql><![CDATA[
SELECT 
	ERR_MSG_CD
FROM COM_ERR_MSG
WHERE	ERR_MSG_CD = @[err_msg_cd]
			]]></sql>
			<params>
				<param name="err_msg_cd" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
